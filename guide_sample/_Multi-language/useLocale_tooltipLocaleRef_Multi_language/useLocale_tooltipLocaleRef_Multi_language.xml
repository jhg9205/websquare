<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:w2="http://www.inswave.com/websquare" xmlns:xf="http://www.w3.org/2002/xforms">
    <head>
    	<w2:type>DEFAULT</w2:type>
        <w2:buildDate/>
        <xf:model>
            <xf:instance>
                <data xmlns=""/>
            </xf:instance>
            <w2:dataCollection baseNode="map">
            	<w2:dataList baseNode="list" repeatNode="map" id="dataList1" saveRemovedData="true">
            		<w2:columnInfo>
            			<w2:column id="col1" name="name1" dataType="text"></w2:column>
            			<w2:column id="col2" name="name2" dataType="text"></w2:column>
            		</w2:columnInfo>
            	</w2:dataList>
            </w2:dataCollection>
            <w2:workflowCollection>
            </w2:workflowCollection>
        </xf:model>
        <style>
            
        </style>
         <script type="text/javascript" lazy="false"><![CDATA[
        
	scwin.onpageload = function() {
		dataList1.setData([1,'a',2,'b',3,'c',4,'d',5,'e']);
	};
	
	scwin.onpageunload = function() {

	};	
	
	scwin.trigger1_onclick = function(e) {
	};

	
	]]></script>
    </head>
    <body ev:onpageload="scwin.onpageload" ev:onpageunload="scwin.onpageunload">
    	<!-- need useLocale property -->
    	<xf:group tagname="table" style="width:50%;" id="" class="w2tb">
    		<w2:attributes>
    			<w2:summary></w2:summary>
    		</w2:attributes>
    		<xf:group tagname="caption"></xf:group>
    		<xf:group tagname="colgroup">
    			<xf:group tagname="col" style="width:13.00%;"></xf:group>
    			<xf:group tagname="col" style="width:29.00%;"></xf:group>
    		</xf:group>
            <xf:group tagname="tr" style="">
                <xf:group tagname="th" style="" class="w2tb_th">Anchor<w2:attributes>
                        <w2:scope>row</w2:scope>
                    </w2:attributes></xf:group>             
                <xf:group tagname="td" style="padding: 10px;" class="w2tb_td">
                    <w2:anchor id="" style="color: red;" toolTip="anchor" outerDiv="false" class="" localeRef="anchor" tooltipLocaleRef="tooltip" useLocale="true" tooltipFormatter="" tooltip="Test for multi-language">
                        <xf:label><![CDATA[]]></xf:label>
                    </w2:anchor>
                </xf:group>
            </xf:group>
            <xf:group tagname="tr" style="">
                <xf:group tagname="th" style="" class="w2tb_th">CheckBox<w2:attributes>
                        <w2:scope>row</w2:scope>
                    </w2:attributes></xf:group>                
                <xf:group tagname="td" style="padding: 10px;padding-bottom: 60px;" class="w2tb_td">
                    <xf:select tooltipDisplay="true" selectedindex="-1" id="" class="" appearance="full"
                    	style="width: 80%;height: 21px;color: red;" cols="1" rows="" ref="" toolTip="checkbox" localeRef="test"
                    	tooltipLocaleRef="tooltip" useLocale="true" useItemLocale="true" tooltip="Test for multi-language">
                    	<xf:choices>
                    		<xf:item>
                    			<xf:label><![CDATA[001]]></xf:label>
                    			<xf:value><![CDATA[aaa]]></xf:value>
                    		</xf:item>
                    		<xf:item>
                    			<xf:label><![CDATA[002]]></xf:label>
                    			<xf:value><![CDATA[bbb]]></xf:value>
                    		</xf:item>
                    		<xf:item>
                    			<xf:label><![CDATA[003]]></xf:label>
                    			<xf:value><![CDATA[ccc]]></xf:value>
                    		</xf:item>
                    		<xf:item>
                    			<xf:label><![CDATA[004]]></xf:label>
                    			<xf:value><![CDATA[ddd]]></xf:value>
                    		</xf:item>
                    	</xf:choices>
                    </xf:select>
                </xf:group>
            </xf:group>
    		<xf:group tagname="tr" style="">
    			<xf:group tagname="th" style="" class="w2tb_th">Trigger<w2:attributes>
    					<w2:scope>row</w2:scope>
    				</w2:attributes></xf:group>    			
    			<xf:group tagname="td" style="padding: 10px;" class="w2tb_td">
                    <xf:trigger type="button" style="width:110px;height: 23px;color: red;" id="" toolTip="trigger" localeRef="trigger" tooltipLocaleRef="tooltip" useLocale="true" tooltip="Test for multi-language">
                        <xf:label><![CDATA[]]></xf:label>
                    </xf:trigger>         
                </xf:group>
    		</xf:group>
    		<xf:group tagname="tr" style="">
    			<xf:group tagname="th" style="" class="w2tb_th">TextBox<w2:attributes>
    					<w2:scope>row</w2:scope>
    				</w2:attributes></xf:group>    			
    			<xf:group tagname="td" style="padding: 10px;" class="w2tb_td" tooltip="">
                    <w2:textbox label="" id="" style="width:110px;color: red;" class="" toolTip="textbox" localeRef="textbox" tooltipLocaleRef="tooltip" useLocale="true" tooltip="Test for multi-language"></w2:textbox>         
                </xf:group>
    		</xf:group>
    		<xf:group tagname="tr" style="">
    			<xf:group tagname="th" style="" class="w2tb_th">Output<w2:attributes>
    					<w2:scope>row</w2:scope>
    				</w2:attributes></xf:group>    			
    			<xf:group tagname="td" style="padding: 10px;" class="w2tb_td" tooltip="">
                    <xf:output style="width:120px;color: red;" id="" toolTip="output" localeRef="output" tooltipLocaleRef="tooltip" useLocale="true" tooltip="Test for multi-language">
                        <xf:label></xf:label>
                    </xf:output>         
                </xf:group>
    		</xf:group>
    	</xf:group>
    </body>
</html>
